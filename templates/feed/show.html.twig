{% extends 'base.html.twig' %}

{% block title %}Feed{% endblock %}

{% block body %}
    <h1>Feed</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feed.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ feed.name }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ feed.url }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_feed_index') }}">back to list</a>

    <a href="{{ path('app_feed_edit', {'id': feed.id}) }}">edit</a>

    {{ include('feed/_delete_form.html.twig') }}
    <hr>
    <div {{ stimulus_controller('feed', {'id':feed.id}) }}></div>
{% endblock %}
